@import 'button'
@import 'window'

body
	margin: 0
	font: 14px "Lucida Grande", Helvetica, Arial, sans-serif
a
	color: #00B7FF

#main-container
	position:absolute;
	top: 0
	left: 0
	right: 0
	bottom: 0
	display: block

canvas#mainview
	position: absolute;
	top: 0
	left: 0
	z-index: 0

#mainpanel
	text-align: center
	position: absolute
	bottom: 0
	left: 50%
	margin-left: -64px
	z-index: 1

